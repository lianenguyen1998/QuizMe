<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.quizme_layout">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon_quizme"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon_quizme"
        android:supportsRtl="true"
        android:theme="@style/Theme.QuizMe_Layout">
        <activity android:name=".Minigame3_pressTheButton"></activity>
        <activity android:name=".Einstellung" />
        <activity android:name=".HintergrundTest" />
        <activity android:name=".Infotext" />
        <activity android:name=".Highscoreliste" />
        <activity
            android:name=".Quiz"
            android:parentActivityName=".Startseite" />
        <activity android:name=".Startseite">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>